{
    "rabbit_version": "3.8.9",
    "users": [
    {
        "name": "guest",
        "password": "guest",
        "tags": "administrator"
    }
    ],
    "vhosts": [
    {
        "name": "/"
    }
    ],
    "permissions": [
    {
        "user": "guest",
        "vhost": "/",
        "configure": ".*",
        "write": ".*",
        "read": ".*"
    }
    ],
    "policies": [],
    "queues": [
        {
            "name": "task_queue",
            "vhost": "/",
            "durable": true,
            "auto_delete": false,
            "arguments": {}
        },
        {
            "name": "done_queue",
            "vhost": "/",
            "durable": true,
            "auto_delete": false,
            "arguments": {}
        }
        ],
        "exchanges": [
        {
            "name": "default_exchange",
            "vhost": "/",
            "type": "direct",
            "durable": true,
            "auto_delete": false,
            "internal": false,
            "arguments": {}
        }
        ],
        "bindings": [
        {
            "source": "default_exchange",
            "vhost": "/",
            "destination": "task_queue",
            "destination_type": "queue",
            "routing_key": "task_queue",
            "arguments": {}
        }
    ]
}